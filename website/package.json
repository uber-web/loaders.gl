{
  "name": "ocular-website-gatsby",
  "version": "0.0.0",
  "description": "A website for Ocular, built with Ocular (Gatsby version)",
  "license": "MIT",
  "keywords": [
    "ocular"
  ],
  "main": "index.js",
  "scripts": {
    "start": "yarn clean && yarn develop",
    "build": "yarn clean-examples && yarn clean && gatsby build",
    "clean": "rm -rf ./.cache ./public",
    "clean-examples": "find ../examples -name node_modules -exec rm -r {} \\; || true",
    "develop": "yarn clean-examples && gatsby develop",
    "serve": "gatsby serve",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d public"
  },
  "dependencies": {
    "@deck.gl/core": "^7.3.0-alpha.7",
    "@deck.gl/geo-layers": "^7.3.0-alpha.7",
    "@deck.gl/layers": "^7.3.0-alpha.7",
    "@deck.gl/mesh-layers": "^7.3.0-alpha.7",
    "@deck.gl/react": "^7.3.0-alpha.7",
    "@loaders.gl/draco": "^1.3.0-beta.1",
    "@loaders.gl/las": "1.3.0-beta.1",
    "@luma.gl/addons": "^7.3.0-alpha.8",
    "@luma.gl/constants": "^7.3.0-alpha.8",
    "@luma.gl/core": "^7.3.0-alpha.8",
    "@luma.gl/webgl": "^7.3.0-alpha.8",
    "@math.gl/culling": "^3.0.0-beta.3",
    "@math.gl/geospatial": "^3.0.0-beta.3",
    "@probe.gl/stats-widget": "^3.1.0-beta.3",
    "math.gl": "^3.0.0-beta.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-map-gl": "^5.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "gatsby": "^2.13.51",
    "gatsby-plugin-no-sourcemaps": "^2.0.2",
    "gh-pages": "^2.1.0",
    "ocular-gatsby": "^1.0.2",
    "sharp": "^0.23.0"
  }
}
