{
  "name": "@loaders.gl/terrain",
  "version": "3.0.0-alpha.4",
  "description": "Framework-independent loader for terrain raster formats",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud",
    "OBJ"
  ],
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-worker && npm run build-bundle && npm run build-bundle -- --env.dev",
    "build-bundle": "webpack --display=minimal --config ../../scripts/webpack/bundle.js",
    "build-worker": "webpack --entry ./src/terrain-loader.worker.js --output ./dist/terrain-loader.worker.js --config ../../scripts/webpack/worker.js && webpack --entry ./src/quantized-mesh-loader.worker.js --output ./dist/quantized-mesh-loader.worker.js --config ../../scripts/webpack/worker.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loaders.gl/loader-utils": "3.0.0-alpha.4",
    "@mapbox/martini": "^0.2.0"
  }
}
