{
  "name": "@loaders.gl/i3s",
  "version": "3.0.0-alpha.19",
  "description": "i3s .",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "i3s",
    "tile",
    "mesh"
  ],
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-worker",
    "build-bundle": "webpack --display=minimal --config ../../scripts/webpack/bundle.js",
    "build-worker": "webpack --entry ./src/workers/i3s-content-worker.js --output ./dist/i3s-content-worker.js --config ../../scripts/webpack/worker.js"
  },
  "dependencies": {
    "@loaders.gl/core": "3.0.0-alpha.19",
    "@loaders.gl/draco": "3.0.0-alpha.19",
    "@loaders.gl/images": "3.0.0-alpha.19",
    "@loaders.gl/loader-utils": "3.0.0-alpha.19",
    "@loaders.gl/textures": "3.0.0-alpha.19",
    "@loaders.gl/tiles": "3.0.0-alpha.19",
    "@luma.gl/constants": "^8.3.0",
    "@math.gl/core": "^3.3.0",
    "@math.gl/culling": "^3.3.0",
    "@math.gl/geospatial": "^3.3.0",
    "probe.gl": "^3.3.0"
  }
}
