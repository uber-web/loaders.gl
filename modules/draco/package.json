{
  "name": "@loaders.gl/draco",
  "version": "2.0.0-alpha.3",
  "description": "Framework-independent loader and writer for Draco compressed meshes and point clouds",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-web/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud",
    "draco"
  ],
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/es6/index.js",
  "sideEffects": false,
  "browser": {
    "fs": false
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "worker-loader.js"
  ],
  "scripts": {
    "pre-build": "npm run copy-libs && npm run build-worker && npm run build-bundle && npm run build-bundle -- --env.dev",
    "copy-libs": "cp -rf ./src/libs ./dist/libs",
    "build-bundle": "webpack --display=minimal --config ../../scripts/bundle.config.js",
    "build-worker": "webpack --entry ./src/draco-loader.worker.js --output ./dist/draco-loader.worker.js --config ../../scripts/worker-webpack-config.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loaders.gl/loader-utils": "2.0.0-alpha.3",
    "draco3d": "^1.3.4"
  }
}
