{
  "name": "loaders.gl",
  "version": "0.4.0",
  "description": "Framework-independent loaders for 3D graphics formats",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "point cloud",
    "GLB",
    "OBJ",
    "PLY",
    "LAZ",
    "PCD",
    "KML"
  ],
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/es6/index.js",
  "files": [
    "src",
    "dist",
    ".bin"
  ],
  "bin": {
    "glbdump" : "./.bin/glbdump"
  },
  "sideEffects": false,
  "browser": {
    "fs": false,
    "text-encoding": false
  },
  "dependencies": {
    "d3-request": "^1.0.6",
    "draco3d": "^1.3.3",
    "text-encoding": "^0.6.4",
    "webgl-obj-loader": "^1.1.3"
  }
}
