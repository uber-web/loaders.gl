{
  "name": "@loaders.gl/images",
  "version": "2.2.1",
  "description": "Framework-independent loaders and writers for images (PNG, JPG, ...)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud",
    "PLY"
  ],
  "types": "src/index.d/ts",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/es6/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle-rollup",
    "build-bundle-webpack": "npm run build-bundle && npm run build-bundle -- --env.dev",
    "build-bundle": "webpack --display=minimal --config ../../scripts/bundle.config.js",
    "build-bundle-rollup": "rollup --config"
  },
  "dependencies": {
    "@loaders.gl/loader-utils": "2.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rollup": "^2.4.0",
    "rollup-plugin-terser": "^5.3.0"
  }
}
