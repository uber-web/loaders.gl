{
  "name": "@loaders.gl/parquet",
  "version": "3.0.0-beta.1",
  "description": "Framework-independent loader for Apache Parquet files",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "parser",
    "table",
    "Parquet",
    "Apache Parquet"
  ],
  "types": "src/index.ts",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle",
    "build-bundle": "webpack --display=minimal --config ../../scripts/webpack/bundle.js",
    "build-worker": "webpack --entry ./src/workers/parquet-worker.js --output ./dist/excel-worker.js --config ../../scripts/webpack/worker.js"
  },
  "browser": {
    "child_process": false,
    "net": false,
    "tls": false,
    "lzo": false
  },
  "dependencies": {
    "@loaders.gl/loader-utils": "3.0.0-beta.1",
    "@loaders.gl/schema": "3.0.0-beta.1",
    "async-mutex": "^0.2.2",
    "brotli": "^1.3.0",
    "bson": "^1.0.4",
    "int53": "^0.2.4",
    "node-int64": "^0.4.0",
    "object-stream": "0.0.1",
    "snappyjs": "^0.6.0",
    "varint": "^5.0.0"
  },
  "optionalDependencies": {
    "lzo": "^0.4.0"
  }
}
